		  String line = args;
	      String pattern = ".*ru";

	      Pattern r = Pattern.compile(pattern);

	      Matcher m = r.matcher(line);
	      if (m.find( )) {
	         return m.group(0).substring(0, m.group(0).length()-3);
	      }else {
	         return "*";
	      }
	}
	
	
